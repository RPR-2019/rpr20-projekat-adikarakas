<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane hgap="20.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints />
          <RowConstraints maxHeight="171.8" minHeight="10.0" vgrow="ALWAYS" />
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="31.19999999999999" vgrow="ALWAYS" />
            <RowConstraints maxHeight="165.2" minHeight="10.0" vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <TableView fx:id="tableViewTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.rowIndex="1" GridPane.rowSpan="3">
              <columns>
                <TableColumn fx:id="tableColumnPosition" prefWidth="40.0" sortable="false" text="Pos" />
                <TableColumn fx:id="tableColumnClubs" prefWidth="220.80001831054688" sortable="false" text="Club" />
                  <TableColumn fx:id="tableColumnPlayed" minWidth="0.0" prefWidth="40.0" sortable="false" text="P" />
                  <TableColumn fx:id="tableColumnWins" minWidth="0.0" prefWidth="40.0" sortable="false" text="W" />
                  <TableColumn fx:id="tableColumnDraws" prefWidth="40.0" sortable="false" text="D" />
                  <TableColumn fx:id="tableColumnLosses" prefWidth="40.0" sortable="false" text="L" />
                  <TableColumn fx:id="tableColumnGoalsScored" prefWidth="40.0" sortable="false" text="GS" />
                  <TableColumn fx:id="tableColumnGoalsConceded" prefWidth="40.0" sortable="false" text="GC" />
                  <TableColumn fx:id="tableColumnGoalDifference" minWidth="0.0" prefWidth="40.0" sortable="false" text="GD" />
                  <TableColumn fx:id="tableColumnPoints" prefWidth="40.0" sortable="false" text="Pts" />
              </columns>
            </TableView>
            <ListView fx:id="fixturesList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="%table" />
            <Label text="%fixtures" GridPane.columnIndex="1" />
            <Label text="%results" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ListView fx:id="resultsList" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <VBox spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="addGameButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addGame" prefHeight="26.0" prefWidth="118.0" text="%addGame" />
                  <Button fx:id="playGameButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#playGame" prefHeight="26.0" prefWidth="118.0" text="%playGame" />
                  <Button fx:id="seeReportButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#seeReport" prefHeight="26.0" prefWidth="118.0" text="%seeReport" />
                  <Button fx:id="statsButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#openStats" prefHeight="26.0" prefWidth="118.0" text="%stats"  />
                  <Button fx:id="finishButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#finish" prefHeight="26.0" prefWidth="118.0" text="%finishSeason" />
               </children>
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
   </center>
   <bottom>
      <HBox prefHeight="19.0" prefWidth="1020.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="statusBarLabel" text="Season in progress" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
